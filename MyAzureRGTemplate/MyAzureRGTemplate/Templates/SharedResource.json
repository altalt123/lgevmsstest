{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "AsiaHKVnetLocation": {
            "type": "string",
            "defaultValue": "East Asia",
            "allowedValues": [
                "East US",
                "West US",
                "West Europe",
                "East Asia",
                "South East Asia"
            ]
        },
        "AsiaJWVNetLocation": {
            "type": "string",
            "defaultValue": "Japan West"
        },
        "USEastVNetLocation": {
            "type": "string",
            "defaultValue": "East US",
            "allowedValues": [
                "East US",
                "West US",
                "West Europe",
                "East Asia",
                "South East Asia"
            ]
        },
        "juleeasiahkstorageName": {
            "type": "string"
        },
        "juleeasiahkstorageType": {
            "type": "string",
            "defaultValue": "Standard_LRS",
            "allowedValues": [
                "Standard_LRS",
                "Standard_GRS",
                "Standard_ZRS"
            ]
        },
        "juleeasiahkstorageLocation": {
            "type": "string",
            "defaultValue": "East Asia",
            "allowedValues": [
                "East US",
                "West US",
