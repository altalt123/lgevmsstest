        "juleeuseaststorageName": {
            "type": "string"
        },
        "juleeuseaststorageType": {
            "type": "string",
            "defaultValue": "Standard_LRS",
            "allowedValues": [
                "Standard_LRS",
                "Standard_GRS",
                "Standard_ZRS"
            ]
        },
        "juleeuseaststorageLocation": {
            "type": "string",
            "defaultValue": "West US",
            "allowedValues": [
                "East US",
                "West US",
                "West Europe",
                "East Asia",
                "South East Asia"
            ]
        }
    },
    "variables": {
        "AsiaHKVnetPrefix": "10.0.0.0/16",
        "AsiaHKVnetSubnet1Name": "DMZ",
